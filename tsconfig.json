{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@/*": [ "src/*" ],
      "@s/*": [ "src/styles/quickskin/*" ],
      "@d/*": [ "src/data/*" ],
      "@ui/*": [ "src/components/ui/*" ],
      "@cv": [ "./cv.json" ]
    }
  }
}
